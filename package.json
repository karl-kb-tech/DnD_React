    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "css-loader": "^2.1.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
